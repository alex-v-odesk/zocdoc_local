
@font-face {
  font-family: 'sharp-sans-bold';
  src: url('https://zocdocstatic.wpengine.com/wp-content/themes/zocdoc/styles/output/desktop/fonts/sharp-sans/SharpSans-No1-Bold.woff') format('woff'); /* Legacy iOS */
  font-stretch: bold;
  font-style: bold;
  font-weight: 900;
}

@font-face {
  font-family: 'sharp-sans-medium';
  src: url('https://zocdocstatic.wpengine.com/wp-content/themes/zocdoc/styles/output/desktop/fonts/sharp-sans/SharpSans-No1-Medium.woff') format('woff'); /* Legacy iOS */
  font-stretch: normal;
  font-style: normal;
  font-weight: 400;
}

@font-face {
  font-family: 'kievit-slab-pro-book';
  src: url('https://zocdocstatic.wpengine.com/wp-content/themes/zocdoc/styles/output/desktop/fonts/kievit-slab//KievitSlabPro-Book.woff') format('woff'); /* Legacy iOS */
  font-stretch: normal;
  font-style: normal;
  font-weight: 500;
}

